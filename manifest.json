  {
    "name": "ElementRemover",
    "version": "1.0",
    "description": "me teame et sa vaatad Romil ;)",
	"permissions": ["activeTab", "declarativeContent", "contextMenus"],
	"background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "content_scripts": [ {
      "js": [ "remover.js" ],
      "matches": [ "<all_urls>" ]
   } ],
	"browser_action": {
      
	  "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
	"icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
	
    "manifest_version": 2
  }